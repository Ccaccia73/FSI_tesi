structural: curr_node, dynamic,
	reference, ROOT, (curr_node - 1) * dL, 0.0, 0.0,
	reference, ROOT, eye, 
	reference, ROOT, null, 
	reference, ROOT, null;

structural: curr_node + 1, dynamic,
	reference, ROOT, curr_node * dL, 0.0, 0.0, 
	reference, ROOT, eye, 
	reference, ROOT, null, 
	reference, ROOT, null;
